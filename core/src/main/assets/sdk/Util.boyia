// Boolean Object And Null
var true = 1, false = 0, null = 0;
// Touch Event
var TOUCH_DOWN = 1, TOUCH_UP = 2, TOUCH_MOVE = 4;
// Util Class
class Util {
    fun log(info) {
        BY_Log(info);
    }
    
    fun json(info) {
        BY_Json(info);
    }
    
    fun instanceof(obj, cls) {
        BY_InstanceOfClass(obj, cls);
    }
    
    fun showToast(info) {
        BY_CallStaticMethod("com/boyia/app/common/utils/BoyiaUtils", "showToast", "(Ljava/lang/String;)V", info);
    }
}